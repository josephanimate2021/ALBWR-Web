{
  "name": "albwr-web",
  "version": "1.0.0",
  "description": "A Node.JS app that runs the albw randomizer cli executables to randomize albw with user input from the GUI without worry of using wasm.",
  "type": "module",
  "scripts": {
    "dev": "node fileSystemForBuildsFolder.js && vite",
    "build": "node fileSystemForBuildsFolder.js && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@webcontainer/api": "latest",
    "@xterm/addon-fit": "latest",
    "@xterm/addon-web-links": "latest",
    "@xterm/xterm": "latest",
    "jquery": "latest",
    "jszip": "latest"
  },
  "devDependencies": {
    "vite": "latest"
  }
}
